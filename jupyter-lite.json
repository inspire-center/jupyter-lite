{
  "jupyter-lite-schema-version": 0,
  "jupyter-config-data": {
    "appName": "My Interactive Notebooks",
    "appVersion": "1.0.0",
    "appUrl": "./",
    "fullAppUrl": "https://YOUR_USERNAME.github.io/my-jupyterlite-demo/",
    "disabledExtensions": [
    "@jupyterlab/help-extension:plugin",
    "@jupyterlab/settingeditor-extension:plugin"
    ],
       "pageConfig": {
           "extraHead": [
             "<style>",
             ".jp-DocumentSubtitle { display: none !important; }",
             ".jp-TrustIndicator { display: none !important; }",
             "</style>"
           ]
         }
  },
  "litePluginSettings": {
    "@jupyterlite/pyodide-kernel-extension:kernel": {
      "pyodideUrl": "https://cdn.jsdelivr.net/pyodide/v0.24.1/full/pyodide.js"
    }
  }
}